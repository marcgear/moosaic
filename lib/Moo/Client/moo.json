{
    "name": "MOO Pack API",
    "apiVersion": "1",
    "baseUrl": "http://www.moo.com/api/service/",
    "description": "MOO Pack API",
    "operations": {
        "getPack":{
            "httpMethod":"GET",
            "responseClass":"\\Moo\\Client\\PackMethodOutput",
            "parameters":{
                "method":{
                    "type":"string",
                    "default":"moo.pack.getPack",
                    "static":true,
                    "required":true,
                    "location":"query"
                },
                "packId": {
                    "description":"packId to fetch",
                    "type":"string",
                    "required":true,
                    "location":"query"
                },
                "includePhysicalSpec": {
                    "description":"include physicalSpec in response",
                    "type":"string",
                    "default":"true",
                    "static":true,
                    "location":"query"
                }
            }
        },
        "getPhysicalSpec":{
            "httpMethod":"GET",
            "responseClass":"\\Moo\\Client\\PhysicalSpecOutput",
            "parameters":{
                "method":{
                    "type":"string",
                    "default":"moo.pack.getPhysicalSpec",
                    "static":true,
                    "required":true,
                    "location":"query"
                },
                "packId": {
                    "description":"packId to fetch",
                    "type":"string",
                    "required":true,
                    "location":"query"
                }
            }
        },
        "updatePhysicalSpec":{
            "httpMethod":"POST",
            "responseClass":"\\Moo\\Client\\PhysicalSpecOutput",
            "parameters":{
                "method":{
                    "type":"string",
                    "default":"moo.pack.updatePhysicalSpec",
                    "static":true,
                    "required":true,
                    "location":"query"
                },
                "packId": {
                    "description":"packId to fetch",
                    "type":"string",
                    "required":true,
                    "location":"query"
                },
                "physicalSpec": {
                    "description":"physical spec to update to",
                    "type":"object",
                    "required":"true",
                    "instanceOf":"\\Moo\\PackModel\\PhysicalSpec",
                    "location":"query",
                    "filters":[
                        {
                            "method":"\\Moo\\Client\\Serializer\\PackModelSerializer::serializePhysicalSpec",
                            "args": ["@value"]
                        }
                    ]
                }
            }
        },
        "createPack":{
            "httpMethod":"POST",
            "responseClass":"\\Moo\\Client\\PackMethodOutput",
            "parameters":{
                "method":{
                    "type":"string",
                    "default":"moo.pack.createPack",
                    "static":true,
                    "required":true,
                    "location":"query"
                },
                "product":{
                    "type":"string",
                    "default":"businesscard",
                    "required":true,
                    "location":"query"
                },
                "includePhysicalSpec": {
                    "description":"include physicalSpec in response",
                    "type":"string",
                    "default":"true",
                    "static":true,
                    "location":"query"
                }
            }
        },
        "updatePack":{},
        "addToCart":{},
        "getTemplate":{},
        "uploadImage":{},
        "importImage":{},
        "measureText":{}
    }
}