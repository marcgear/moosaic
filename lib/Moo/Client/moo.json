{
    "name": "MOO Pack API",
    "apiVersion": "1",
    "baseUrl": "http://www.moo.com/api/service/",
    "description": "MOO Pack API",
    "operations": {
        "getPack":{
            "httpMethod":"GET",
            "responseClass":"Pack",
            "parameters":{
                "method":{
                    "type":"string",
                    "default":"moo.pack.getPack",
                    "static":true,
                    "required":true,
                    "location":"query"
                },
                "packId": {
                    "description":"packId to fetch",
                    "type":"string",
                    "required":true,
                    "location":"query"
                },
                "includePhysicalSpec": {
                    "description":"include physicalSpec in response",
                    "type":"boolean",
                    "location":"query"
                }
            }
        },
        "createPack":{
            "httpMethod":"POST",
            "responseClass":"\\Moo\\Client\\CreatePackOutput",
            "parameters":{
                "method":{
                    "type":"string",
                    "default":"moo.pack.createPack",
                    "static":true,
                    "required":true,
                    "location":"query"
                },
                "product":{
                    "type":"string",
                    "default":"businesscard",
                    "required":true,
                    "location":"query"
                }
            }
        },
        "getPhysicalSpec":{},
        "updatePack":{},
        "updatePhysicalSpec":{},
        "addToCart":{},
        "getTemplate":{},
        "uploadImage":{},
        "importImage":{},
        "measureText":{}
    },
    "models": {
        "CreatePackOutput": {
            "type":"object",
            "properties": {
                "packId": {
                    "location":"json",
                    "type":"string"
                },
                "dropins": {
                    "location":"json",
                    "type":"array",
                    "items":{
                        "type":"string"
                    }
                },
                "pack": {
                    "location":"json",
                    "type":"object",
                    "filters":[
                        {
                            "method":"\\Moo\\Client\\Client::parsePack",
                            "args": [ "@value" ]
                        }
                    ]
                }
            }
        },
        "Pack": {
            "type":"object",
            "additionalProperties": {
                "location":"json"
            }
        }
    }
}